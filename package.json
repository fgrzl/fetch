{
  "name": "@fgrzl/fetch",
  "version": "0.0.0",
  "description": "A simple fetch client with some extra goodies",
  "keywords": [
    "fetch",
    "typescript",
    "api"
  ],
  "homepage": "https://github.com/fgrzl/fetch#readme",
  "bugs": {
    "url": "https://github.com/fgrzl/fetch/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fgrzl/fetch.git"
  },
  "license": "MIT",
  "author": "smiggleworth",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:check": "npm run lint && npm run format:check",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vitest": "^0.5.4",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  }
}
