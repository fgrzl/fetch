{
  "name": "@fgrzl/fetch",
  "version": "1.4.0",
  "description": "A modern, type-safe HTTP client with middleware support for CSRF protection and authentication",
  "keywords": [
    "fetch",
    "typescript",
    "api",
    "http",
    "client",
    "middleware",
    "CSRF",
    "auth",
    "type-safe"
  ],
  "homepage": "https://github.com/fgrzl/fetch#readme",
  "bugs": {
    "url": "https://github.com/fgrzl/fetch/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fgrzl/fetch.git"
  },
  "license": "MIT",
  "author": "smiggleworth",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./min": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.min.js",
      "require": "./dist/cjs/index.min.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/cjs/client/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js",
      "require": "./dist/cjs/middleware/index.js"
    },
    "./middleware/authentication": {
      "types": "./dist/middleware/authentication/index.d.ts",
      "import": "./dist/middleware/authentication/index.js",
      "require": "./dist/cjs/middleware/authentication/index.js"
    },
    "./middleware/authorization": {
      "types": "./dist/middleware/authorization/index.d.ts",
      "import": "./dist/middleware/authorization/index.js",
      "require": "./dist/cjs/middleware/authorization/index.js"
    },
    "./middleware/cache": {
      "types": "./dist/middleware/cache/index.d.ts",
      "import": "./dist/middleware/cache/index.js",
      "require": "./dist/cjs/middleware/cache/index.js"
    },
    "./middleware/csrf": {
      "types": "./dist/middleware/csrf/index.d.ts",
      "import": "./dist/middleware/csrf/index.js",
      "require": "./dist/cjs/middleware/csrf/index.js"
    },
    "./middleware/logging": {
      "types": "./dist/middleware/logging/index.d.ts",
      "import": "./dist/middleware/logging/index.js",
      "require": "./dist/cjs/middleware/logging/index.js"
    },
    "./middleware/rate-limit": {
      "types": "./dist/middleware/rate-limit/index.d.ts",
      "import": "./dist/middleware/rate-limit/index.js",
      "require": "./dist/cjs/middleware/rate-limit/index.js"
    },
    "./middleware/retry": {
      "types": "./dist/middleware/retry/index.d.ts",
      "import": "./dist/middleware/retry/index.js",
      "require": "./dist/cjs/middleware/retry/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js",
      "require": "./dist/cjs/errors/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "sideEffects": false,
  "overrides": {
    "@typescript-eslint/utils": "^8.39.1"
  },
  "scripts": {
    "build": "tsup",
    "build:esm": "tsup --format esm",
    "build:cjs": "tsup --format cjs",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:check": "npm run lint && npm run fmt:check",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vitest": "^0.5.4",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
